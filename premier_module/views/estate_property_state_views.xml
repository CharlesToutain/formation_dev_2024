<odoo>
    <record id="view_estate_property_state_form" model="ir.ui.view">
        <field name="name">estate.property.state.form</field>
        <field name="model">estate.property.state</field>
        <field name="arch" type="xml">
            <form string="Estate Property State">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_estate_property_state_list" model="ir.ui.view">
        <field name="name">estate.property.state.list</field>
        <field name="model">estate.property.state</field>
        <field name="arch" type="xml">
            <list string="Estate Property State">
                <field name="name"/>
            </list>
        </field>
    </record>


    <record id="action_estate_property_state" model="ir.actions.act_window">
        <field name="name">Property States</field>
        <field name="res_model">estate.property.state</field>
        <field name="view_mode">list,form</field>
    </record>

</odoo>